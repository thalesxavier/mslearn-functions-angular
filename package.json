{
  "name": "shifting-express-to-functions",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "server": "ng serve",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "node:build-ts": "tsc -p server",
    "node:watch-ts": "tsc -w -p server",
    "node:lint": "npm run node:build-ts && eslint \"server/**/*.ts\" --quiet --fix",
    "node:debug": "WWW=./dist/vacationwishlist nodemon -r dotenv/config --inspect=9229 server/dist/index.js",
    "node:start": "WWW=./dist/vacationwishlist node -r dotenv/config server/dist/index.js",
    "full:build": "npm run node:build-ts && npm run build",
    "full:debug": "concurrently \"npm run node:debug\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.0.2",
    "@angular/animations": "^13.0.2",
    "@angular/cli": "~13.2.3",
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.1.3",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/core": "^13.0.2",
    "@angular/forms": "^13.0.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.0.2",
    "@angular/router": "^13.2.2",
    "@ngrx/data": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@po-ui/ng-components": "^6.3.0",
    "@po-ui/ng-templates": "^6.3.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "typescript": "~4.5.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}